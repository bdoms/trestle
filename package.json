  },
  "comment": "for browserslist see https://github.com/DeMoorJasper/parcel-plugin-svelte/issues/44",
  "browserslist": [
    "last 1 chrome versions"
  ],
  "dependencies": {},
  "devDependencies": {
    "ava": "2.2.0",
    "browser-env": "3.2.6",
    "esm": "3.2.25",
    "page": "1.11.4",
    "parcel-bundler": "1.12.3",
    "parcel-plugin-svelte": "4.0.1",
    "svelte": "3.7.1"
  },
  "ava": {
    "files": [
      "svelte/tests/**"
    ],
    "require": [
      "./svelte/tests/_browser_env.js",
      "esm"
    ]
  }
}
